{
  "name": "@mimeojs/demo",
  "private": true,
  "version": "0.0.1",
  "description": "Stencil App Starter",
  "scripts": {
    "build": "stencil build --prerender",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "serve": "http-server ./dist/www",
    "clean": "shx rm -rf ./dist *.tsbuildinfo",
    "graphql:codegen": "graphql-codegen --config codegen.yml"
  },
  "devDependencies": {
    "@apollo/client": "^3.3.11",
    "@graphql-codegen/cli": "^1.21.1",
    "@graphql-codegen/typescript": "^1.21.0",
    "@graphql-codegen/typescript-document-nodes": "^1.17.9",
    "@graphql-codegen/typescript-operations": "^1.17.14",
    "@mimeojs/cli": "file:..\\cli",
    "@stencil/core": "^2.4.0",
    "@stencil/helmet": "^0.3.3-0",
    "apollo-link-rest": "^0.8.0-beta.0",
    "shx": "^0.3.3",
    "stencil-router-v2": "file:..\\stencil-router"
  },
  "license": "MIT"
}
